{
	"require": {
		"admin-columns/admin-columns-pro": "*",
		"admin-columns/ac-addon-acf": "*",
		"advanced-custom-fields/advanced-custom-fields-pro": "*",
		"wpackagist-plugin/wp-multibyte-patch": "*",
		"wpackagist-plugin/jetpack": "*"
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://composer.admincolumns.com"
		},
		{
			"type": "composer",
			"url": "https://pivvenit.github.io/acf-composer-bridge/composer/v3/wordpress-plugin/"
		},
		{
			"type": "composer",
			"url": "https://wpackagist.org",
			"only": [
				"wpackagist-plugin/*",
				"wpackagist-theme/*"
			]
		}
	],
	"extra": {
		"installer-paths": {
			"source/wp-content/plugins/{$name}/": [
				"type:wordpress-plugin"
			],
			"source/wp-content/themes/{$name}/": [
				"type:wordpress-theme"
			]
		}
	},
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"pivvenit/acf-pro-installer": true
		}
	}
}
