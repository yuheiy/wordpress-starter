import { Alpine } from "alpinejs";
<% for (const dep of dependencies) { -%>
import <%- camelCase(dep.name) %> from "./<%- relative(replaceExt(dep.path, "")) %>";
<% } -%>

export default function (Alpine: Alpine) {
<% for (const dep of dependencies) { -%>
	Alpine.store("<%- camelCase(dep.name) %>", <%- camelCase(dep.name) %>());
<% } -%>
}
